{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ManagementPermissions",
      "Effect": "Allow",
      "Action": [
        "s3:CreateBucket",
        "s3:DeleteBucket",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning",
        "s3:GetBucketEncryption",
        "s3:GetBucketPublicAccessBlock",
        "s3:GetBucketPolicy",
        "s3:GetBucketNotification",
        "s3:GetBucketLogging",
        "s3:GetBucketCors",
        "s3:GetBucketWebsite",
        "s3:GetBucketTagging",
        "s3:GetBucketAcl",
        "s3:ListBucket",
        "s3:PutBucketVersioning",
        "s3:PutBucketEncryption",
        "s3:PutBucketPublicAccessBlock",
        "s3:PutBucketPolicy",
        "s3:PutBucketNotification",
        "s3:PutBucketLogging",
        "s3:PutBucketCors",
        "s3:PutBucketWebsite",
        "s3:PutBucketTagging",
        "s3:PutBucketAcl",
        "s3:DeleteBucketPolicy",
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObjectVersion",
        "s3:DeleteObjectVersion"
      ],
      "Resource": "*"
    },
