{
  "accounts": {
    "123456789012": {
      "name": "production",
      "environments": {
        "prod": {
          "iam_policies": {
            "developer_policy": {
              "name": "DeveloperAccess-Prod",
              "description": "Developer access policy for production environment",
              "document": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":[\"s3:GetObject\",\"s3:ListBucket\"],\"Resource\":[\"arn:aws:s3:::prod-app-bucket/*\",\"arn:aws:s3:::prod-app-bucket\"]}]}",
              "tags": {
                "Environment": "prod",
                "Team": "engineering"
              }
            },
            "readonly_policy": {
              "name": "ReadOnlyAccess-Prod",
              "description": "Read-only access policy for production environment",
              "document": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":[\"s3:GetObject\",\"s3:ListBucket\",\"cloudwatch:GetMetricStatistics\",\"logs:FilterLogEvents\"],\"Resource\":\"*\"}]}",
              "tags": {
                "Environment": "prod",
                "Team": "operations"
              }
            }
          },
          "iam_roles": {
            "app_execution_role": {
              "name": "AppExecutionRole-Prod",
              "description": "Execution role for production applications",
              "assume_role_policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Principal\":{\"Service\":\"ec2.amazonaws.com\"},\"Action\":\"sts:AssumeRole\"}]}",
              "tags": {
                "Environment": "prod"
              }
            }
          },
          "cross_account_role": "ProductionAccountAccessRole",
          "region": "us-east-1"
        }
      }
    },
    "234567890123": {
      "name": "staging", 
      "environments": {
        "staging": {
          "iam_policies": {
            "developer_policy": {
              "name": "DeveloperAccess-Staging",
              "description": "Developer access policy for staging environment",
              "document": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":[\"s3:*\",\"ec2:DescribeInstances\",\"rds:DescribeDBInstances\"],\"Resource\":\"*\"}]}",
              "tags": {
                "Environment": "staging",
                "Team": "engineering"
              }
            },
            "tester_policy": {
              "name": "TesterAccess-Staging",
              "description": "Tester access policy for staging environment", 
              "document": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":[\"s3:GetObject\",\"s3:PutObject\",\"s3:ListBucket\"],\"Resource\":[\"arn:aws:s3:::staging-test-bucket/*\",\"arn:aws:s3:::staging-test-bucket\"]}]}",
              "tags": {
                "Environment": "staging",
                "Team": "qa"
              }
            }
          },
          "iam_roles": {
            "app_execution_role": {
              "name": "AppExecutionRole-Staging",
              "description": "Execution role for staging applications",
              "assume_role_policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Principal\":{\"Service\":\"ec2.amazonaws.com\"},\"Action\":\"sts:AssumeRole\"}]}",
              "tags": {
                "Environment": "staging"
              }
            }
          },
          "cross_account_role": "StagingAccountAccessRole",
          "region": "us-east-1"
        }
      }
    },
    "345678901234": {
      "name": "development",
      "environments": {
        "dev": {
          "iam_policies": {
            "developer_policy": {
              "name": "DeveloperAccess-Dev", 
              "description": "Developer access policy for development environment",
              "document": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Action\":\"*\",\"Resource\":\"*\"}]}",
              "tags": {
                "Environment": "dev",
                "Team": "engineering"
              }
            }
          },
          "iam_roles": {
            "app_execution_role": {
              "name": "AppExecutionRole-Dev",
              "description": "Execution role for development applications",
              "assume_role_policy": "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Effect\":\"Allow\",\"Principal\":{\"Service\":[\"ec2.amazonaws.com\",\"lambda.amazonaws.com\"]},\"Action\":\"sts:AssumeRole\"}]}",
              "tags": {
                "Environment": "dev"
              }
            }
          },
          "cross_account_role": "DevelopmentAccountAccessRole",
          "region": "us-west-2"
        }
      }
    }
  }
}